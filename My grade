a = int(input("enter Mathematics degree: "))
b = int(input("enter My degree is Arabic: "))
c = int(input("enter Science degree: "))
d = int(input("enter English degree: "))
e = int(input("enter Sociology degree: "))
f = int(input("enter The degree of standardization: "))
g = int(input("enter Jurisprudence degree: "))
h = int(input("enter Hadith degree: "))
i = int(input("enter Degree of interpretation: "))
j = int(input("enter Quran degree: "))


Mathematics = (a*5)
Arabic = (b*6)
Science = (c*4)
English = (d*4)
Sociology = (e*3)
standardization = (f*2)
Jurisprudence = (g*2)
Hadith = (h*1)
interpretation = (i*2)
Quran = (j*2)
print(Mathematics)
print(Arabic)
print(Science)
print(English)
print(Sociology)
print(standardization)
print(Jurisprudence)
print(Hadith)
print(interpretation)
print(Quran)
print()
print()







hack = (Mathematics + Arabic + Science + English + Sociology + standardization + Jurisprudence + Hadith + interpretation + Quran)
Toiital = (hack/17)
print("Toiital: " + str(Toiital))
print()
print()
print()
print()
print()



hackpython12 = (Total + Toital + Toiital)
print(hackpython12)
print()
print()
print()
print()
print(100 - hackpython12)
